{
    "compilerOptions": {
        "target": "es2017",
        "declaration": true,
        "rootDir": "./src",
        "removeComments": true,
        "strict": true,
        "outDir": "./dist",
        "noImplicitAny": false,
        "baseUrl": "./src",
        "esModuleInterop": true,
        "module": "commonjs", 
        "moduleResolution": "node",
        "resolveJsonModule": true
    },
    "include": ["src", "src/utils/Config.ts"],
}